SELECT 
    p.id,
    p.factura_id,
    f.numero as numero_factura,
    p.fecha_pago,
    p.monto,
    p.metodo_pago,
    p.observaciones,
    c.nombre_encargado
FROM pagos_cliente p
JOIN facturas f ON p.factura_id = f.id
JOIN clientes c ON f.cliente_id = c.id
ORDER BY p.fecha_pago DESC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
